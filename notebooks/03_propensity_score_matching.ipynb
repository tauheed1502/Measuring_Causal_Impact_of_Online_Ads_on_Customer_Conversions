   "source": [
    "# Test different calipers\n",
    "calipers = [0.05, 0.1, 0.15, 0.2]\n",
    "matching_results = {}\n",
    "\n",
    "print(\"=== PROPENSITY SCORE MATCHING RESULTS ===\")\n",
    "print(f\"{'Caliper':<8} {'ATE':<8} {'Matches':<8} {'Rate':<8}\")\n",
    "print(\"-\" * 35)\n",
    "\n",
    "for caliper in calipers:\n",
    "    matched_data, psm_result = propensity_score_matching(\n",
