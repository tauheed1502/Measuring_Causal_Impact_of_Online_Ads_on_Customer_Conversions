    "\n",
    "# Select optimal caliper (balance between matches and quality)\n",
    "optimal_caliper = 0.1\n",
    "matched_data = matching_results[optimal_caliper]['matched_data']\n",
    "psm_final = matching_results[optimal_caliper]['results']\n",
