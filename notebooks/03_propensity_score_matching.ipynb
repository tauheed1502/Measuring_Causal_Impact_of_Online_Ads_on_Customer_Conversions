   "source": [
    "# Propensity Score Matching Analysis\n",
    "\n",
    "This notebook implements propensity score matching to:\n",
    "- Estimate propensity scores\n",
    "- Perform 1:1 matching\n",
    "- Assess covariate balance after matching\n",
    "- Calculate treatment effects"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import sys\n",
    "sys.path.append('..')\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from src.data.preprocessing import preprocess, balance_check\n",
    "from src.data.feature_engineering import engineer_features, select_confounders\n",
    "from src.models.propensity_score import PropensityScoreModel, propensity_score_matching\n",
    "from src.evaluation.causal_metrics import compute_ate, lift_calculation\n",
    "from src.utils.plotting import plot_propensity_distribution\n",
    "\n",
    "# Load data from previous notebook\n",
    "df = pd.read_csv('../data/processed/explored_ad_data.csv')\n",
    "print(f\"Loaded {len(df)} records\")\n",
    "\n",
    "# Preprocess and engineer features\n",
    "df_processed = preprocess(df)\n",
    "df_features = engineer_features(df_processed)\n",
    "confounders = select_confounders(df_features)\n",
    "\n",
    "print(f\"Selected confounders: {confounders}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Step 1: Propensity Score Estimation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Train propensity score model\n",
    "ps_model = PropensityScoreModel('logistic')\n",
    "ps_results = ps_model.train_propensity_model(df_features[confounders], df_features['treatment'])\n",
    "\n",
    "print(f\"Propensity model performance:\")\n",
    "print(f\"  Cross-validation AUC: {ps_results['cv_auc_mean']:.3f} Â± {ps_results['cv_auc_std']:.3f}\")\n",
    "\n",
    "# Add propensity scores to dataframe\n",
    "df_features['propensity_score'] = ps_results['propensity_scores']\n",
    "\n",
    "# Check overlap\n",
    "overlap_check = ps_model.check_overlap()\n",
    "print(f\"\\nOverlap assessment:\")\n",
    "print(f\"  Propensity range: [{overlap_check['min_propensity']:.3f}, {overlap_check['max_propensity']:.3f}]\")\n",
    "print(f\"  Extreme scores: {overlap_check['extreme_low_count']} low, {overlap_check['extreme_high_count']} high\")\n",
    "print(f\"  Overlap adequate: {overlap_check['overlap_ok']}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Step 2: Visualize Propensity Score Distributions"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create propensity score visualizations\n",
    "fig, axes = plt.subplots(2, 2, figsize=(15, 10))\n",
    "fig.suptitle('Propensity Score Analysis', fontsize=16)\n",
    "\n",
    "# Distribution by treatment group\n",
    "treated_ps = df_features[df_features['treatment']==1]['propensity_score']\n",
    "control_ps = df_features[df_features['treatment']==0]['propensity_score']\n",
    "\n",
    "axes[0,0].hist(control_ps, bins=30, alpha=0.7, label='Control', color='blue', density=True)\n",
    "axes[0,0].hist(treated_ps, bins=30, alpha=0.7, label='Treated', color='red', density=True)\n",
    "axes[0,0].set_xlabel('Propensity Score')\n",
    "axes[0,0].set_ylabel('Density')\n",
    "axes[0,0].set_title('Propensity Score Distribution')\n",
    "axes[0,0].legend()\n",
    "\n",
    "# Box plot by treatment\n",
    "df_features.boxplot(column='propensity_score', by='treatment', ax=axes[0,1])\n",
    "axes[0,1].set_title('Propensity Score by Treatment Group')\n",
    "axes[0,1].set_xlabel('Treatment Group')\n",
    "\n",
    "# Propensity vs outcome\n",
    "ps_bins = pd.qcut(df_features['propensity_score'], q=10, labels=False)\n",
    "outcome_by_ps = df_features.groupby([ps_bins, 'treatment'])['conversion'].mean().unstack()\n",
    "outcome_by_ps.plot(ax=axes[1,0], marker='o')\n",
    "axes[1,0].set_title('Conversion Rate by Propensity Decile')\n",
    "axes[1,0].set_xlabel('Propensity Decile')\n",
    "axes[1,0].set_ylabel('Conversion Rate')\n",
    "\n",
    "# Overlap assessment\n",
    "axes[1,1].scatter(df_features['propensity_score'], df_features['conversion'], \n",
    "                 c=df_features['treatment'], alpha=0.6, cmap='RdYlBu')\n",
    "axes[1,1].set_xlabel('Propensity Score')\n",
    "axes[1,1].set_ylabel('Conversion')\n",
    "axes[1,1].set_title('Overlap Assessment')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
